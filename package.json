    "@types/supertest": "^6.0.2",
    "dotenv-cli": "^7.4.2",
    "jest": "^29.7.0",
    "prisma": "^5.15.0",
    "rimraf": "^5.0.7",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@prisma/client": "^5.15.0",
